<dataset-name>%(identifier)s</dataset-name>
<input-directory>%(input-directory)s</input-directory>
<output-directory>%(output-directory)s</output-directory>

<deformation-seed>%(seed-path)s</deformation-seed>

<deformation-iterations>%(deformation-iterations)i</deformation-iterations>
<deformation-initial-pressure>%(deformation-initial-pressure)f</deformation-initial-pressure>
<deformation-kappa>%(deformation-kappa)f</deformation-kappa>

<infiltration-iterations>%(infiltration-iterations)i</infiltration-iterations>
